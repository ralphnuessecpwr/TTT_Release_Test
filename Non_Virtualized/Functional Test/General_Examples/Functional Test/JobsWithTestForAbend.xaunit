    <Job name="Job" jobName="HDDRXM0J" jobLocation="JOBDEFINITION">
      <Content><![CDATA[//HDDRXM0J JOB ('EUDD,INTL'),'NUESSE',NOTIFY=&SYSUID,
//             MSGLEVEL=(1,1),MSGCLASS=X,CLASS=A,REGION=6M
/*JOBPARM S=${lparName}
