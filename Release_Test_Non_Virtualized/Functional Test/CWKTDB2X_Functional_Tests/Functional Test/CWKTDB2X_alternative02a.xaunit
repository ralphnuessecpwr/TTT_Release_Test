<?xml version="1.0" encoding="UTF-8"?>
<XaUnit xmlns="http://www.xact.dk/xatester/v/1" xmlns:data="http://www.xact.dk/xatester/data/v/1" componentNameUnderTest="cwktdb2x">
  <Test>
    <Job name="Load DB2" datasetName="hddrxm0.demo.cwc2.util.jcl(lodktbef)" jobLocation="DATASET">
      <Content><![CDATA[//XXX DD DSN=]]></Content>
    </Job>
    <Component label="CWKTDB2X" moduleName="CWKTDB2X" programExitVerify="value &lt; 8" rollback="true">
      <Data>
        <parminfo>
          <parm-lth>5</parm-lth>
          <parm-data>00001</parm-data>
          <grand-total expected="7160" />
          <total-record expected="8" />
        </parminfo>
      </Data>
      <Setup>
        <Content><![CDATA[//EMPFILE  DD DISP=SHR,DSN=HDDRXM0.DEMO.CWC2.CWKTDATA
//RPTFILE  DD DISP=SHR,DSN=HDDRXM0.DEMO.CWC2.KTDEMO.RPTFILE
//*RPTFILE  DD SYSOUT=*]]></Content>
      </Setup>
      <SQLSelect id="selectAfter" maxRows="20">
        <Statement>SELECT EMP_NUM,  WAGE_TYPE,  REGION,  FIRST_LAST_NAME,  STREET_ADDR,  CITY,  STATE,  ZIP,  HIREDATE,  HOURS,  RATE,  WAGES,  OVERTIME,  COMM&#xD;
 FROM HDDRXM0.KT_DEMOTAB</Statement>
        <Output name="EMP_NUM" />
        <Output name="WAGE_TYPE" />
        <Output name="REGION" />
        <Output name="FIRST_LAST_NAME" />
        <Output name="STREET_ADDR" />
        <Output name="CITY" />
        <Output name="STATE" />
        <Output name="ZIP" />
        <Output name="HIREDATE" />
        <Output name="HOURS" />
        <Output name="RATE" />
        <Output name="WAGES" />
        <Output name="OVERTIME" />
        <Output name="COMM" />
      </SQLSelect>
    </Component>
    <ReadRemoteFile fileName="HDDRXM0.DEMO.CWC2.KTDEMO.RPTFILE" id="rptfileActual" label="Read report file" firstRecord="10" numberRecords="8" mDataDefinitionName="rptEmpDet" />
    <IterateData id="loopThroughRptfileActual" referencedId="rptfileActual" dataPath="">
      <Variable id="empTotalExpect">
        <data:Replaceable>
          <Expression>
            <Statement><![CDATA[var tmpWages = Number(loopThroughRptfileActual.record['employee-dtl']['emp-dtl-wages'])
var tmpOt    = Number(loopThroughRptfileActual.record['employee-dtl']['emp-dtl-ot'])
var tmpComm  = Number(loopThroughRptfileActual.record['employee-dtl']['emp-dtl-comm'])
var tmpTotal = tmpWages + tmpOt + tmpComm

tmpTotal.toFixed(2)]]></Statement>
          </Expression>
        </data:Replaceable>
      </Variable>
      <Variable id="empTotal">
        <data:Replaceable>
          <Expression>
            <Statement><![CDATA[Number(loopThroughRptfileActual.record['employee-dtl']['emp-dtl-total']).toFixed(2)]]></Statement>
          </Expression>
        </data:Replaceable>
      </Variable>
      <Verify name="Verify" haltExecution="false">
        <Expected expression="empTotalExpect" />
        <Actual expression="empTotal" />
      </Verify>
    </IterateData>
    <FileAIDCompare label="File-AID Compare" checkChanged="true" checkInserted="true" checkDeleted="true">
      <JCL><![CDATA[//FASTEP   EXEC PGM=FILEAID,REGION=08M
//STEPLIB  DD DISP=SHR,DSN=SYS2.CW.VJR17Y.CXVJLOAD
//         DD DISP=SHR,DSN=SYS2.CW.VJR17Y.SXVJLOAD
//SYSPRINT DD  SYSOUT=*
//SYSLIST  DD  SYSOUT=*
//DD01     DD  DSN=HDDRXM0.DEMO.CWC2.KTDEMO.RPTFILE.EXPECT,
//             DISP=SHR
//DD01C    DD  DSN=HDDRXM0.DEMO.CWC2.KTDEMO.RPTFILE,
//             DISP=SHR
//DD01CP   DD  DSN=HDDRXM0.DEMO.FA.COMPCRIT(CWXTCOB),
//             DISP=SHR                              
//DD01RL   DD  DSN=HDDRXM0.DEMO.COPY,
//             DISP=SHR
//DD01XR   DD  DSN=HDDRXM0.DEMO.XREF(RPTFILE),
//             DISP=SHR
//SYSIN    DD  *
$$DD01 COMPARE
/*]]></JCL>
    </FileAIDCompare>
    <Variable id="db2Expected" filePath="Data/KT_DEMO_expected.xml" />
    <IterateData id="loopThroughSelectAfter" referencedId="selectAfter" dataPath="">
      <Verify name="Verify" haltExecution="false">
        <Expected expression="Number(db2Expected.Row[loopThroughSelectAfterIndex].WAGES).toFixed(2)" />
        <Actual expression="tmpWages = Number(loopThroughSelectAfter.Row.WAGES) / 100&#xD;&#xA;tmpWages.toFixed(2)" />
      </Verify>
    </IterateData>
  </Test>
  <MetaData>
    <MDataDefinition name="rptEmpDet">
      <employee-dtl native="01 EMPLOYEE-DTL.">
        <filler1 type="Character" length="1" bytes="1" native="05 FILLER PIC X VALUE SPACES." />
        <emp-dtl-name type="Character" length="15" bytes="15" native="05 EMP-DTL-NAME PIC X(15)." />
        <filler2 type="Character" length="1" bytes="1" native="05 FILLER PIC X VALUE SPACES." />
        <emp-dtl-region type="Character" length="5" bytes="5" native="05 EMP-DTL-REGION PIC X(5)." />
        <filler3 type="Character" length="3" bytes="3" native="05 FILLER PIC X(3) VALUE SPACES." />
        <emp-dtl-type type="Character" length="1" bytes="1" native="05 EMP-DTL-TYPE PIC X." />
        <filler4 type="Character" length="3" bytes="3" native="05 FILLER PIC X(3) VALUE SPACES." />
        <emp-dtl-hire-mm type="NumericDisplay" length="2" bytes="2" native="05 EMP-DTL-HIRE-MM PIC 9(2)." signed="false" decimals="0" />
        <emp-dtl-slash1 type="Character" length="1" bytes="1" native="05 EMP-DTL-SLASH1 PIC X VALUE SPACES." />
        <emp-dtl-hire-dd type="NumericDisplay" length="2" bytes="2" native="05 EMP-DTL-HIRE-DD PIC 9(2)." signed="false" decimals="0" />
        <emp-dtl-slash2 type="Character" length="1" bytes="1" native="05 EMP-DTL-SLASH2 PIC X VALUE SPACES." />
        <emp-dtl-hire-yy type="NumericDisplay" length="2" bytes="2" native="05 EMP-DTL-HIRE-YY PIC 9(2)." signed="false" decimals="0" />
        <filler5 type="Character" length="3" bytes="3" native="05 FILLER PIC X(3) VALUE SPACES." />
        <emp-dtl-yrs-of-service type="NumericDisplay" length="2" bytes="2" native="05 EMP-DTL-YRS-OF-SERVICE PIC 9(2)." signed="false" decimals="0" />
        <filler6 type="Character" length="2" bytes="2" native="05 FILLER PIC X(2) VALUE SPACES." />
        <emp-dtl-wages type="NumericEdited" length="8" bytes="8" native="05 EMP-DTL-WAGES PIC ZZZZ9.99." signed="false" decimals="0" />
        <filler7 type="Character" length="1" bytes="1" native="05 FILLER PIC X VALUE SPACES." />
        <emp-dtl-ot type="NumericEdited" length="8" bytes="8" native="05 EMP-DTL-OT PIC ZZZZ9.99." signed="false" decimals="0" />
        <filler8 type="Character" length="2" bytes="2" native="05 FILLER PIC X(2) VALUE SPACES." />
        <emp-dtl-comm type="NumericEdited" length="8" bytes="8" native="05 EMP-DTL-COMM PIC ZZZZ9.99." signed="false" decimals="0" />
        <filler9 type="Character" length="1" bytes="1" native="05 FILLER PIC X VALUE SPACES." />
        <emp-dtl-total type="NumericEdited" length="8" bytes="8" native="05 EMP-DTL-TOTAL PIC ZZZZ9.99." signed="false" decimals="0" />
      </employee-dtl>
    </MDataDefinition>
    <Component name="CWKTDB2X" type="COBOL">
      <INOUT>
        <parminfo>
          <parm-lth type="NumericBinary" length="4" bytes="2" signed="true" decimals="0" />
          <parm-data type="Character" length="5" bytes="5" />
          <grand-total type="NumericBinary" length="7" bytes="4" signed="true" decimals="0" />
          <total-record type="NumericDisplay" length="3" bytes="3" signed="false" decimals="0" />
        </parminfo>
      </INOUT>
    </Component>
  </MetaData>
</XaUnit>
