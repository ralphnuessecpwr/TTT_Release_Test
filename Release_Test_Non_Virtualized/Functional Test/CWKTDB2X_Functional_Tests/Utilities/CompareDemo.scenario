<?xml version="1.0" encoding="UTF-8"?>
<XaUnit xmlns="http://www.xact.dk/xatester/v/1" xmlns:data="http://www.xact.dk/xatester/data/v/1">
  <Test>
    <SQLSelect id="sql" maxRows="20">
      <Statement>select RIDENT,FIRSTNAME from XATESTER.XAEMPLOYEE</Statement>
      <Output name="RIDENT" />
      <Output name="FIRSTNAME" />
    </SQLSelect>
    <Variable id="expected" filePath="expected.xml" />
    <Verify name="Verify" haltExecution="false">
      <Expected expression="expected" />
      <Actual expression="sql" />
    </Verify>
  </Test>
</XaUnit>
