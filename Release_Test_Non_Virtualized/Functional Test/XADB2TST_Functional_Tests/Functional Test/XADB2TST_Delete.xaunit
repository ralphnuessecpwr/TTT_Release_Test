        </SQLSelect>
        <Verify name="Status Code" haltExecution="false" description="Status Code is expected to be 'D'">
          <Expected expression="expectedStatuscode" />
          <Actual expression="selectAfter.Row.STATUSCODE" />
        </Verify>
      </Scope>
