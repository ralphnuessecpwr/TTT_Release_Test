  (
  RIDENT, 
  FIRSTNAME, 
  LASTNAME, 
  USERID, 
  CPRNR, 
  STATUSCODE, 
  TSTMP
  )
VALUES 
  (
  '${newRowId}',
  '${oldFirstname}',
  '${oldLastname}',
  '${oldUserid}',
  ${oldCprnr},
  'O',
  current timestamp
  );]]></Statement>
      </SQLUpdate>
    </Scope>
